<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<context id="context1">
		<!-- 基于oracle的分页插件 -->
		<plugin type="org.mybatis.generator.plugins.PagingByOraclePlugin"></plugin>
		<!-- JDBC -->
		<jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
			connectionURL="jdbc:oracle:thin:@192.168.101.17:1521:ORCL"
			userId="htgl" password="htgl" />
		<!-- Xxx.java -->
		<javaModelGenerator targetPackage="com.cattsoft.cms.xsgl.entity"
			targetProject="E:\workspaces\generation\src\main\java">
			<property name="rootClass"
				value="com.cattsoft.baseplatform.core.entity.Entity" />
		</javaModelGenerator>
		<!-- XxxMapper.xml -->
		<sqlMapGenerator targetPackage="com.cattsoft.cms.xsgl.persistence"
			targetProject="E:\workspaces\generation\src\main\resources" />
